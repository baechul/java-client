        },
        "totalPages": {
          "type": "integer"
        },
        "page": {
          "type": "integer"
        }
      }
    },
    "Transaction": {
      "type": "object",
      "properties": {
        "accountId": {
          "type": "string"
        },
        "transactionId": {
          "type": "string"
        },
        "postedTimestamp": {
          "type": "string",
          "format": "date-time"
        },
        "transactionTimestamp": {
          "type": "string",
          "format": "date-time"
        },
        "description": {
          "type": "string"
        },
        "status": {
          "type": "string",
          "enum": [
            "POSTED"
          ],
          "description": "Only 'POSTED' transactions are returned"
        },
        "amount": {
          "type": "number"
        },
        "debitCreditMemo": {
          "type": "string"
        }
      }
    },
    "DepositTransaction": {
      "type": "object",
      "allOf": [
        {
          "$ref": "#/definitions/Transaction"
        },
        {
          "properties": {
            "transactionType": {
              "type": "string"
            },
            "checkNumber": {
              "type": "integer"
            }
          }
        }
      ]
    },
    "Currency": {
      "type": "object",
      "properties": {
        "currencyRate": {
          "type": "number"
        },
        "currencyCode": {
          "type": "string",
          "description": "Currency code from Iso4217Code"
        },
        "originalCurrencyCode": {
          "type": "string",
          "description": "Currency code from Iso4217Code"
        }
      }
    }
  }
